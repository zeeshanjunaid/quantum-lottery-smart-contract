# RPC and deployer key
# For Arbitrum Sepolia, set your node endpoint here.
RPC_URL=
PRIVATE_KEY=

# Core constructor args
# USDC address (6 decimals). For testing, deploy TestUSDC and paste its address here.
USDC_ADDRESS=

# Where protocol fees are sent
TREASURY_ADDRESS=

# Chainlink VRF (Arbitrum Sepolia examples below)
# Coordinator: example from broadcasts (421614)
VRF_COORDINATOR=0x41034422FA37197F49965a383501229671542475

# VRF Subscription ID (uint64). Create and fund via Chainlink VRF and paste your ID here.
VRF_SUBSCRIPTION_ID=

# Gas lane (key hash): example from broadcasts (421614)
GAS_LANE=0x114f3da0a805b8a67d6a7a05154f14afd91811821b38e09e03ade28a24ac2166

# Optional: set a default chain alias
# CHAIN_ID=421614
